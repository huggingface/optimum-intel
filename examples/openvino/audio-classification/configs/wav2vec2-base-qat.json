{
    "algorithm": "quantization",
    "quantize_inputs": false,
    "preset": "mixed",
    "overflow_fix": "enable",
    "initializer": {
        "range": {
            "num_init_samples": 300,
            "type": "mean_min_max"
        },
        "batchnorm_adaptation": {
            "num_bn_adaptation_samples": 0
        }
    },
    "scope_overrides": {
        "activations": {
            "{re}.*matmul_0": {
                "mode": "symmetric"
            },
            "{re}.*scaled_dot_product_attention_0": {
                "mode": "symmetric"
            }
        }
    },
    "ignored_scopes": [
        "{re}.*feature_extractor.*",
        "{re}.*__add___[0-1]",
        "{re}.*layer_norm_0"
    ]
}